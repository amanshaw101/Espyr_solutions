/*
10.1 Printing Star Pattern (N Rows) 
***** 
***** 
***** 
***** 
. 
. 
N rows
*/
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int nor;
    cout<<"enter number of rows: ";
    cin>>nor;
    for(int i=0;i<nor;i++){
        printf("*****\n");
    }
    return 0;
}
