/*
10.2 Printing Number Pattern (N Rows) 
11111 
22222 
33333 
44444 
. 
. 
N rows 
*/
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int nor;
    cout<<"enter number of rows: ";
    cin>>nor;
    for(int i=1;i<=nor;i++){
        for(int j=1;j<=nor;j++){
            cout<<i;
    }
        cout<<endl;
    }
    return 0;
}



/*
10.3 Printing Number Pattern (N Rows) 
12345 
12345 
12345 
12345 
. 
. 
N rows
*/
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int nor;
    cout<<"enter number of rows: ";
    cin>>nor;
    for(int i=0;i<nor;i++){
        cout<<"12345"<<endl;
    }
    return 0;
}
